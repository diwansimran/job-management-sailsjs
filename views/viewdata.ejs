<body>
    <div>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item">
              <a class="nav-link" href="/">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/jobs562">View Data</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/adddata562">Add Data</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/finddata562">Find Data</a>
            </li>
           
          </ul>
        </div>
    <h1 class="text-center"> Displaying all jobs </h1>
    <br>
    <table class="table table-striped">
    <thead class="thead-dark">
    <tr>
    <th> Job Name </th>
    <th> Part Id </th>
    <th> Qty </th>
    <th> Update </th>
    <th> Delete </th>
    </tr>
    </thead>
    <tbody>
    <% jobs562.forEach(function(jobs562){ %>
    <tr>
    <td> <%= jobs562.jobName %> </td>
    <td> <%= jobs562.id %> </td>
    <td> <%= jobs562.qty %> </td>
    <td> <form action="/updatejob562/" method="POST">
        <input type="hidden" name="jobName" value="<%= jobs562.jobName %>" />
        <input type="hidden" name="id" value="<%= jobs562.id %>" />
        <input type="hidden" name="qty" value="<%= jobs562.qty %>" />
        <button class="btn btn-outline-info"> Update </button>
        </form></td>
    <td> <form action="/deletejob562/" method="POST">
        <input type="hidden" name="jobName" value="<%= jobs562.jobName %>" />
        <input type="hidden" name="id" value="<%= jobs562.id %>" />        
        <button class="btn btn-outline-danger"> Delete </button>
        </form></td>
    </tr>
    <% }) %>
    </tbody>
    </table>
    </body>
    
</body>